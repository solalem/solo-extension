{
	"name": "solo",
	"displayName": "Solo Modeling",
	"description": "Solo Modeling",
	"version": "0.0.1",
	"publisher": "solalem",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"engines": {
		"vscode": "^1.71.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:featureDesigns",
		"onView:codeTree",
		"onLanguage:json",
		"onLanguage:jsonc",
		"onCommand:testView.reveal"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "solo-explorer",
					"title": "Solo Explorer",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"solo-explorer": [
				{
					"id": "codeTree",
					"name": "Code Tree View"
				},
				{
					"id": "featureDesigns",
					"name": "Feature Designs",
					"icon": "media/dep.svg",
					"contextualTitle": "Feature Designs"
				}
			]
		},
		"commands": [
			{
				"command": "featureDesigns.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "featureDesigns.previewCode",
				"title": "Preview Code",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "featureDesigns.addEntry",
				"title": "Add"
			},
			{
				"command": "featureDesigns.editEntry",
				"title": "Edit",
				"icon": {
					"light": "resources/light/edit.svg",
					"dark": "resources/dark/edit.svg"
				}
			},
			{
				"command": "featureDesigns.deleteEntry",
				"title": "Delete"
			},
			{
				"command": "codeTree.refreshFile",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "codeTree.openFile",
				"title": "Open File"
			},
			{
				"command": "codeTree.changeTitle",
				"title": "Change Title"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "codeTree.revealResource"
				}
			],
			"view/title": [
				{
					"command": "featureDesigns.refreshEntry",
					"when": "view == featureDesigns",
					"group": "navigation"
				},
				{
					"command": "featureDesigns.addEntry",
					"when": "view == featureDesigns"
				},
				{
					"command": "codeTree.refresh",
					"when": "view == codeTree",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "featureDesigns.editEntry",
					"when": "view == featureDesigns && viewItem == dependency",
					"group": "inline"
				},
				{
					"command": "featureDesigns.deleteEntry",
					"when": "view == featureDesigns && viewItem == dependency"
				},
				{
					"command": "codeTree.refreshFile",
					"when": "view == codeTree && viewItem == file",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/ftp": "^0.3.33",
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^16.11.7",
		"@types/rimraf": "^2.0.2",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.13.0",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"ftp": "^0.3.10",
		"jsonc-parser": "^0.4.2",
		"minimist": "^1.2.6",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2"
	}
}
